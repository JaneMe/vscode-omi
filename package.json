{
	"name": "omi-develop",
	"displayName": "omi-develop",
	"description": "Omi for Visual Studio Code (develop)",
	"license": "Omi",
	"version": "1.0.4",
	"publisher": "ZainChen",
	"author": {
		"name": "ZainChen"
	},
	"icon": "assets/omi-logo2019-128.png",
	"homepage": "https://github.com/ZainChen/vscode-omi/blob/dev/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/ZainChen/vscode-omi"
	},
	"bugs": {
		"url": "https://github.com/ZainChen/vscode-omi/issues"
	},
	"keywords": [
		"zain",
		"ZainChen",
		"omi",
		"志银",
		"tencent",
		"nodejs"
	],
	"categories": [
		"Snippets",
		"Other"
	],
	"engines": {
		"vscode": "^1.31.0"
	},
	"activationEvents": [
		"onView:omi.view.ecosystem",
		"onView:fileExplorer",
		"onView:omi.nodeDependencies",
		"onView:jsonOutline",
		"onCommand:omi.cmd.ecoRefresh",
		"onCommand:omi.cmd.ecoRefreshNode",
		"onCommand:omi.cmd.help",
		"onCommand:omi.cmd.webview",
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:css",
		"onLanguage:html",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:markdown",
		"onLanguage:php",
		"onLanguage:python",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onLanguage:objective-c",
		"onLanguage:xml",
		"onLanguage:sql",
		"onLanguage:java",
		"onLanguage:swift",
		"onLanguage:tex",
		"onLanguage:go",
		"onLanguage:csharp"
	],
	"main": "./src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "omi.cmd.ecoRefresh",
				"title": "omi refresh ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.ecoRefreshNode",
				"title": "omi refresh node ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.help",
				"title": "omi help",
				"icon": {
					"light": "./assets/omi-set.svg",
					"dark": "./assets/omi-set.svg"
				}
			},
			{
				"command": "omi.cmd.webview",
				"title": "omi wv",
				"icon": {
					"light": "./assets/add1.svg",
					"dark": "./assets/add1.svg"
				}
			},
			{
				"command": "fileExplorer.openFile",
				"title": "Open File"
			},
			{
				"command": "nodeDependencies.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "nodeDependencies.addEntry",
				"title": "Add"
			},
			{
				"command": "nodeDependencies.editEntry",
				"title": "Edit",
				"icon": {
					"light": "./assets/light/edit.svg",
					"dark": "./assets/dark/edit.svg"
				}
			},
			{
				"command": "nodeDependencies.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "jsonOutline.refresh",
				"title": "Refresh",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "jsonOutline.refreshNode",
				"title": "Refresh",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "jsonOutline.renameNode",
				"title": "Rename"
			},
			{
				"command": "extension.demo.showWelcome",
				"title": "显示自定义欢迎页"
			}
		],
		"languages": [
			{
				"id": "json",
				"extensions": [
					".json"
				],
				"aliases": [
					"Json"
				]
			},
			{
				"id": "javascript",
				"extensions": [
					".js"
				],
				"aliases": [
					"JavaScript",
					"js",
					"JS",
					"Js"
				]
			},
			{
				"id": "tex",
				"extensions": [
					".txt"
				],
				"aliases": [
					"text"
				]
			}
		],
		"snippets": [
			{
				"path": "./src/snippets/omi.code-snippets"
			},
			{
				"language": "javascript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "typescript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "json",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "html",
				"path": "./src/snippets/omi.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "omi-views",
					"title": "omi",
					"icon": "./assets/omi-logo2019-30-30.svg"
				}
			]
		},
		"views": {
			"omi-views": [
				{
					"id": "omi.view.ecosystem",
					"name": "ecosystem"
				},
				{
					"id": "fileExplorer",
					"name": "File Explorer"
				},
				{
					"id": "jsonOutline",
					"name": "json",
					"when": "jsonOutlineEnabled"
				},
				{
					"id": "nodeDependencies",
					"name": "Node Dependencies"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "omi.cmd.help",
					"when": "editorFocus",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "markdown.showPreview",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "omi.cmd.help",
					"group": "navigation"
				},
				{
					"command": "markdown.showPreview",
					"when": "resourceLangId == markdown",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"view/title": [
				{
					"command": "omi.cmd.ecoRefresh",
					"when": "view == omi.view.ecosystem",
					"group": "navigation"
				},
				{
					"command": "nodeDependencies.refreshEntry",
					"when": "view == nodeDependencies",
					"group": "navigation"
				},
				{
					"command": "nodeDependencies.addEntry",
					"when": "view == nodeDependencies"
				},
				{
					"command": "jsonOutline.refresh",
					"when": "view == jsonOutline",
					"group": "navigation"
				},
				{
					"command": "omi.cmd.help",
					"when": "view == omi.view.ecosystem",
					"group": "navigation"
				},
				{
					"command": "omi.cmd.webview",
					"when": "view == omi.view.ecosystem"
				}
			],
			"view/item/context": [
				{
					"command": "omi.cmd.ecoRefreshNode",
					"when": "view == omi.view.ecosystem",
					"group": "inline"
				},
				{
					"command": "nodeDependencies.editEntry",
					"when": "view == nodeDependencies && viewItem == dependency",
					"group": "inline"
				},
				{
					"command": "nodeDependencies.deleteEntry",
					"when": "view == nodeDependencies && viewItem == dependency"
				},
				{
					"command": "jsonOutline.renameNode",
					"when": "view == jsonOutline"
				},
				{
					"command": "jsonOutline.refreshNode",
					"when": "view == jsonOutline",
					"group": "inline"
				}
			]
		},
		"keybindings": [
			{
				"command": "omi.cmd.help",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Omi",
			"properties": {
				"omi.author.dntzhang": {
					"type": "string",
					"default": "Welcome to omi.",
					"description": "Omi for Visual Studio Code"
				},
				"jsonOutline.autorefresh": {
					"type": "boolean",
					"default": false,
					"description": "是否根据 json 文件内容，实时更新菜单！"
				},
				"omi.start.welcome": {
					"type": "boolean",
					"default": false,
					"description": "是否在每次启动时显示欢迎提示！"
				},
				"vscodePluginDemo.showTip": {
					"type": "boolean",
					"default": true,
					"description": "启动时显示自定义欢迎页"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"cheerio": "^1.0.0-rc.2",
		"request": "^2.88.0",
		"request-promise": "^4.2.4"
	}
}
